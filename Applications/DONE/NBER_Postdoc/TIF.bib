@article{drucker_local_2020,
  title = {Do Local Governments Use Business Tax Incentives to Compensate for High Business Property Taxes?},
  author = {Drucker, Joshua and Funderburg, Richard and Merriman, David and Weber, Rachel},
  year = {2020},
  month = mar,
  journal = {Regional Science and Urban Economics},
  volume = {81},
  pages = {103498},
  issn = {01660462},
  doi = {10.1016/j.regsciurbeco.2019.103498},
  urldate = {2022-03-29},
  abstract = {Why do municipalities set business property taxes higher than the costs of business services when, in competitive markets, this would result in the inefficient provision of public goods? Statutory tax rates may be set artificially high to allow selective reductions for targeted firms through incentives. We examine the nearly 2500 tax codes\textemdash sub-municipal geographic areas\textemdash that host business locations in 134 municipalities in Cook County, Illinois. We explain spatial variation in tax incentives as a function of the relative competitiveness of tax codes. Our findings suggest that municipalities apply property tax abatements to offset their own relatively uncompetitive tax rates but use Tax Increment Financing (TIF) incentives to engage in vertical competition in order to capture revenue from overlapping governments.},
  langid = {english}
}

