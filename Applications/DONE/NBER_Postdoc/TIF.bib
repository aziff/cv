@article{dye_effects_2000,
  title = {The {{Effects}} of {{Tax Increment Financing}} on {{Economic Development}}},
  author = {Dye, Richard F. and Merriman, David F.},
  year = {2000},
  month = mar,
  journal = {Journal of Urban Economics},
  volume = {47},
  number = {2},
  pages = {306--328},
  issn = {00941190},
  doi = {10.1006/juec.1999.2149},
  urldate = {2022-03-29},
  abstract = {Local governments attempt to influence business location decisions and economic development through use of the property tax. Tax increment financing (TIF) sequesters property tax revenues that result from growth in assessed valuation. The TIF revenues are to be used for economic development projects but may also be diverted for other purposes. We have constructed an extensive data set for the Chicago metropolitan area that includes information on property value growth before and after TIF adoption. In contrast to the conventional wisdom, we find evidence that cities that adopt TIF grow more slowly than those that do not. We test for and reject sample selection bias as an explanation of this finding. We argue that our empirical finding is plausible and present a theoretical argument explaining why TIF might reduce municipal growth.},
  langid = {english}
}

